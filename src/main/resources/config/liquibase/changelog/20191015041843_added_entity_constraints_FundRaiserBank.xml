<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity FundRaiserBank.
    -->
    <changeSet id="20191015041843-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="fund_raiser_id"
                                 baseTableName="fund_raiser_bank"
                                 constraintName="fk_fund_raiser_bank_fund_raiser_id"
                                 referencedColumnNames="id"
                                 referencedTableName="fund_raiser"/>

        <addForeignKeyConstraint baseColumnNames="bank_id"
                                 baseTableName="fund_raiser_bank"
                                 constraintName="fk_fund_raiser_bank_bank_id"
                                 referencedColumnNames="id"
                                 referencedTableName="bank"/>

        <addForeignKeyConstraint baseColumnNames="currency_id"
                                 baseTableName="fund_raiser_bank"
                                 constraintName="fk_fund_raiser_bank_currency_id"
                                 referencedColumnNames="id"
                                 referencedTableName="currency"/>

    </changeSet>
</databaseChangeLog>
