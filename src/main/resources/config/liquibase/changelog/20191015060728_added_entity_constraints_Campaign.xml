<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">
    <!--
        Added the constraints for entity Campaign.
    -->
    <changeSet id="20191015060728-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="currency_id"
                                 baseTableName="campaign"
                                 constraintName="fk_campaign_currency_id"
                                 referencedColumnNames="id"
                                 referencedTableName="currency"/>

        <addForeignKeyConstraint baseColumnNames="fund_raiser_id"
                                 baseTableName="campaign"
                                 constraintName="fk_campaign_fund_raiser_id"
                                 referencedColumnNames="id"
                                 referencedTableName="fund_raiser"/>

    </changeSet>
</databaseChangeLog>
